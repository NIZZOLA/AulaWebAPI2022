{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "LoggerBasePath": "c:\\logs\\webapi01",
  "LoggerFileTemplate": "[{Timestamp:yyyy-MM-dd HH:mm:ss.ffff}][{Level}] - WebApi01 - {Message}{NewLine}{Exception}",
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "MinimumLevel": "Error",
    "WriteTo": [ "Console", "File" ]
  },
  "AllowedHosts": "*"
}
